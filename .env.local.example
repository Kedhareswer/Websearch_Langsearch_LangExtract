# Copy to .env.local and fill in your credentials
LANGSEARCH_API_KEY=your_actual_api_key_here
# Optionally override base URL if Langsearch provides a different one
LANGSEARCH_BASE_URL=https://api.langsearch.com

# Google Gemini API key for LangExtract (required for AI summaries)
GEMINI_API_KEY=your_gemini_api_key_here

# Python service URL (default is localhost:5000)
PYTHON_SERVICE_URL=http://localhost:5000
